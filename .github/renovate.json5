{
	"$schema": "https://docs.renovatebot.com/renovate-schema.json",
	"extends": [
		"config:recommended",
	],
	packageRules: [
		{
			groupName: 'Ktlint',
			matchPackageNames: [
				'com.pinterest.ktlint:*',
			]
		},
		{
			groupName: 'OpenRewrite',
			matchPackageNames: [
				'org.openrewrite.recipe:*',
			],
		},
	]
}
